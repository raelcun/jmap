{
  "name": "jmap",
  "version": "0.1.0",
  "license": "MPL-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/raelcun/jmap.git"
  },
  "bugs": {
    "url": "https://github.com/raelcun/jmap/issues"
  },
  "author": {
    "name": "Dan Taylor"
  },
  "keywords": [],
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier --write '**/*.{tsx,ts,jsx,js,json}'",
    "lint:fix": "eslint './{src,server}/**/*.{ts,tsx}' --fix",
    "start": "run-p --race start:server start:app",
    "start:app": "react-scripts start",
    "start:server": "cd server && env-cmd -f .env.local ts-node-dev --no-notify --respawn --transpileOnly ./server.ts && wait-on tcp:4000",
    "test": "react-scripts test"
  },
  "browserslist": {
    "development": [
      ">5%",
      "not dead"
    ],
    "production": [
      ">5%",
      "not dead"
    ]
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "jira-client": "^6.14.0",
    "mermaid": "^8.11.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "~3.7.2",
    "wait-on": "^4.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/express": "^4.17.2",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^24.0.0",
    "@types/jira-client": "^6.13.1",
    "@types/mermaid": "^8.2.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1"
  }
}